
<style>
    body {
   font-family:Ubuntu-Regular,sans-serif;
}


</style>

<div class="mainBoxBlog">
    
        
        <div class="leftcolumn">
            {{#if blogContent}}
            {{#each blogContent}}
            <div class="blogCard">
                
                <h2>{{this.title}}</h2>
               
                <div class="fakeimg" style="height:350px;"><img class="blogImg" src="/blogImg/{{this._id}}.jpg" alt=""></div>
                
                <p style="white-space: pre-line;">{{this.content}}</p>
                
            </div>
            {{/each}}
            {{else}}
            <div class="tableNotFound">
                <h3>Sorry something went wrong</h3>
                <div>
                    <img class="notFoundPng" src="/images/empty-folder (2).png" alt="">
                </div>
            </div>
            {{/if}}
        </div>
    
    
</div>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
